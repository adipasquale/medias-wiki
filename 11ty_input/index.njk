---
layout: default_layout
bodyClass: large
---

<form>
  <label for="autocomplete">Recherche</label>
  <div class="autocomplete-form">
    <div>
      <input id="autocomplete" />
    </div>
    <div>
      <button id="clear-autocomplete" disabled="disabled">Effacer</button>
    </div>
  </div>
</form>

<div class="filterable-section">
  <h2 id="medias">Liste des m√©dias</h2>
  <ul class="large-list">
    {% for media in collections.media %}
      <li class="filterable" data-entity-nom="{{ media.data.nom }}">
        <a href="/medias/{{ media.data.slug }}/">
          {{ media.data.nom }}
        </a>
      </li>
    {% endfor %}
  </ul>
</div>

<div class="filterable-section">
  <h2 id="proprietaires">Liste des propri√©taires</h2>
  <ul class="large-list">
    {% for proprietaire in collections.proprietaire %}
      <li class="filterable" data-entity-nom="{{ proprietaire.data.nom }}">
        {% if proprietaire.data.nom %}
          <a href="/proprietaires/{{ proprietaire.data.slug }}/">
            {{ proprietaire.data.nom }}
          </a>
        {% else %}
          {{ proprietaire.fileSlug }}
        {% endif %}

        {% if proprietaire.data.typeLibelle != "Personne physique" %}
          üè¢
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</div>

<script src="js/fuse.min.js"></script>
<script src="/js/search.js"></script>
